{
  "manifest_version": 2,
  "name": "Close Meeting Tabs",
  "version": "1.2",

  "description": "Automatically closes Zoom, LiveShare, Webex, and Pragli meeting browser tabs for you",

  "permissions": [
    "tabs",
	"alarms"
  ],

  "background":{
	  "scripts": ["background-script.js"]
  },

  "options_ui":{
	"page": "settings/options.html",
	"browser_style": true,
	"chrome_style": true
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "maki.gr@gmail.com"
    }
  }
}
